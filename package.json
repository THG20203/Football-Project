  },
  "homepage": "https://github.com/THG20203/Football-Project#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}
